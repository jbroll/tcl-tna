#!/usr/bin/env tclkit8.6
#

package require tcltest
lappend auto_path lib

package require tna

tna::debug 1
tna::nthread 1

tna::array create a double 4000 4000
tna::array create b double 4000 4000
tna::array create c double 4000 4000

tna::nthread 4


tna::expr {
    a = 4
    b = 6
    c = 8
    a = a*b*c + a*b*c + a*b*c + a*b*c + a*b*c
}
